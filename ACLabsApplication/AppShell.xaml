<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="ACLabsApplication.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:ACLabsApplication"
    Shell.FlyoutBehavior="Disabled">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" >
        <Shell.TitleView>
            <StackLayout>
                <ImageButton Source="home.png" Clicked="GoToHomePageButton_Clicked"></ImageButton>
            </StackLayout>
        </Shell.TitleView>
    </ShellContent>
</Shell>
